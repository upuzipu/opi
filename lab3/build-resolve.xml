<?xml version="1.0" encoding="UTF-8"?>
<!-- Bootstrap: загрузка зависимостей без ant-contrib. Запустите первым: ant -f build-resolve.xml resolve -->
<project name="lab3-resolve" default="resolve" xmlns:ivy="antlib:org.apache.ivy.ant">
    <property file="build.properties"/>
    <target name="resolve">
        <mkdir dir="${lib}"/>
        <ivy:retrieve pattern="lib/[artifact].[ext]" conf="jars"/>
    </target>
</project>
